{
    "name": "deniztezcan/laravel-strava",
    "description": "A laravel package to access data from the Strava API.",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "DenizTezcan\\Strava\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DenizTezcan\\Strava\\StravaServiceProvider"
            ],
            "aliases": {
                "Strava": "DenizTezcan\\Strava\\StravaFacade"
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Richie McMullen",
            "email": "richiemcmullen007@gmail.com"
        },
        {
            "name": "Deniz Tezcan",
            "email": "howdy@deniztezcan.me"
        }
    ],
    "minimum-stability": "dev"
}
